{
  "name": "txi-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "docker:local": "docker-compose -f docker-compose.local.yml up -d txi_local_database",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed-clients.ts",
    "seed:operators": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed-operators.ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.34",
    "@hookform/resolvers": "^3.1.1",
    "@json2csv/plainjs": "^7.0.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "@tanstack/react-table": "^8.9.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "accordion": "^3.0.2",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.14",
    "awesome-phonenumber": "^5.10.0",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "framer-motion": "^10.12.21",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-next-dynamic-ts": "^0.1.1",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "lucide-react": "^0.256.0",
    "nanoid": "^4.0.2",
    "next": "13.4.5",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.4",
    "npx": "^10.2.2",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "18.2.0",
    "react-google-autocomplete": "^2.7.3",
    "react-hook-form": "^7.45.1",
    "react-number-format": "^5.2.2",
    "shadcn-ui": "^0.3.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "next-sitemap": "^4.1.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
